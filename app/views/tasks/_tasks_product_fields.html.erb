<li class="nested-fields">
	<%= f.label :product_id, "Nome do Produto", :class => 'control-label col-lg-4'%>
	<div class="col-lg-6">
		<%= autocomplete_field_tag "product_name", "", autocomplete_product_name_tasks_path, class: 'form-control', :id_element => 'meuid'%>
		<%= f.hidden_field(:product_id, value: "0")%>
	</div>
	<%= f.label :file_name, "Nome Arquivo:", :class => 'control-label col-lg-4'%>
	<div class="col-lg-6">
		<%= f.text_field :file_name, class: 'form-control'%>
	</div>
	<%= f.label :quantity, "Quantidade:", :class => 'control-label col-lg-4'%>
	<div class="col-lg-6">
		<%= f.number_field :quantity, class: 'form-control' %>
	</div>
	<div class="col-lg-12 text-center">
		<%= link_to_remove_association "Remover Produto", f%>
	</div>
</li>